C,cAAuD;AAAxC,IAAA,IAAI,EAAC,MAAmC;;aAAnB,KAAA,CAAA,M,GAAM,M;MAAa;AAAX,IAAA,QAAQ,EAAR;AAAW,GAApE,E,IAAA,E;;AAAA,G,iBAAiD,KAAA,CAAA,M,GAC7C,CAFN,C,EAGA,W,CA/BF,E;;AAAA,GAiCI,CAlCN,CAmCI,CArCN,C","sourcesContent":["<template>\n  <div class=\"comment-wrapper\">\n    <h1>Add a comment</h1>\n    <div class=\"container\">\n      <form @submit.prevent=\"addComment\">\n        <div class=\"form-group\">\n          <label>Title for comment</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            v-model=\"title\"\n            placeholder=\"Title of comment\"\n          />\n        </div>\n        <div class=\"form-group\">\n          <label>Content of comment</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            v-model=\"content\"\n            placeholder=\"content of comment\"\n          />\n        </div>\n        <div class=\"form-group\">\n          <label>Attached to review id:</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            v-model=\"review_id\"\n            placeholder=\"Insert Id\"\n          />\n        </div>\n        <div class=\"form-group\">\n          <input class=\"form-control\" type=\"text\" v-model=\"userId\" disabled />\n        </div>\n        <button class=\"btn btn-primary btn-block\">Send comment</button>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\nimport axios from \"axios\";\n\nexport default {\n  name: \"AddComment\",\n  data() {\n    return {\n      title: \"\",\n      content: \"\",\n      review_id: \"\",\n      userId: localStorage.getItem(\"userId\"),\n    };\n  },\n  methods: {\n    async addComment() {\n      const response = await axios.post(\"v1/comment\", {\n        title: this.title,\n        content: this.content,\n        review_id: this.review_id,\n        user_id: this.userId,\n      });\n      console.log(response)\n    },\n  },\n    computed: {\n    ...mapGetters(['user'])\n  }\n};\n</script>\n\n<style scoped>\n.comment-wrapper {\n  border: orange solid 4px;\n}\n.container {\n  border: solid 3px green;\n  min-width: fit-content;\n  overflow-wrap: break-word;\n}\n.form-control {\n  width: fit-content;\n}\n</style>"],"sourceRoot":""}]}