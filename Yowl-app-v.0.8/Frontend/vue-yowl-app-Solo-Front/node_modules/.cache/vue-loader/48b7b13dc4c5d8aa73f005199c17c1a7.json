em('email', this.email);\n                localStorage.setItem('userId', response.data.user.id );\n                localStorage.setItem('userName', response.data.user.name);\n                this.$store.dispatch('user', response.data.user);   \n                this.$router.push('/');\n                \n\n\n            }\n        },\n    }\n</script>"]}]}