CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;QACX,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;IACb,EAAE,CAAC","file":"/home/coder01/MyProjects/Test/dev08LaraVue/YOWL-App/Yowl-app-v.0.8/Frontend/vue-yowl-app-Solo-Front/src/components/LoginProvider.vue","sourceRoot":"","sourcesContent":["<template>\n    <div>\n        \n        <form @submit.prevent=\"handleSubmit\">\n            <h3>Login as a Provider</h3>\n\n            <div class=\"form-group\">\n                <label>Email</label>\n                <input type=\"email\" class=\"form-control\" v-model=\"email\" placeholder=\"Email\" required/>\n            </div>\n\n            <div class=\"form-group\">\n                <label>Password</label>\n                <input type=\"password\" class=\"form-control\" v-model=\"password\" placeholder=\"Password\" required/>\n            </div>\n\n            <button class=\"btn btn-primary btn-block\">Login</button>\n        </form>\n        <ButtonLogin/>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport ButtonLogin from './ButtonLogin.vue'\n    export default {\n        name: 'LoginProvider',\n        components:{\n            ButtonLogin\n        },\n        data () {\n            return {\n                email: '',\n                password: '',\n                userId:'',\n                \n            }\n        },\n        methods:{\n            async handleSubmit(){\n\n                const response = await axios.post('/v1/provider/login', {\n                    email: this.email,\n                    password: this.password\n                }).catch(e =>{\n                    console.log(e);\n                });\n                console.log(this.email)\n                console.log(this.password)\n                console.log(response)\n                localStorage.setItem('token', response.data.token);\n                localStorage.setItem('email', this.email)\n                localStorage.setItem('userId', response.data.user.id )\n                this.$store.dispatch('user', response.data.user);\n                this.$router.push('/');\n                \n                \n\n\n            }\n        },\n    }\n</script>\n\n<style scoped>\nform {\n    padding: 30px;\n}\n    /* .auth-inner {\n        margin: 0;\n    } */\n</style>"]}]}