CAAC,CAAC,EAAE;QACX,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;IACb,EAAE,CAAC","file":"/home/coder01/MyProjects/Test/dev08LaraVue/YOWL-App/Yowl-app-v.0.8/Frontend/vue-yowl-app-Solo-Front/src/components/Register.vue","sourceRoot":"","sourcesContent":["<template>\n    <div class=\"register\">\n        \n        <form @submit.prevent=\"handleSubmit\">\n        <h3>Sign Up</h3>\n\n        <div class=\"form-group\">\n            <label>First Name</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"name\" placeholder=\"First Name\" required/>\n        </div>\n\n         <div class=\"form-group\">\n            <label>Last Name</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"surname\" placeholder=\"Last Name\" required/>\n        </div>\n\n         <div class=\"form-group\">\n            <label>Email</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"email\" placeholder=\"Email\" required/>\n        </div>\n\n        <div class=\"form-group\">\n            <label>Password</label>\n            <input type=\"password\" class=\"form-control\" v-model=\"password\" placeholder=\"Password\" required/>\n        </div>\n\n        <div class=\"form-group\">\n            <label>Confirm Password</label>\n            <input type=\"password\" class=\"form-control\" v-model=\"password_confirm\" placeholder=\"Confirm Password\" required/>\n        </div>\n\n        <button class=\"btn btn-primary btn-block\">Sign Up</button>\n        </form>\n        <Button/>\n    </div>\n</template>\n\n<script>\n    import axios from 'axios'\n    import Button from './Button.vue'\n\n    export default{\n        name: 'Register',\n        components:{\n            Button,\n        },\n        data() {\n            return {\n                name: '',\n                surname: '',\n                email: '',\n                password: '',\n                password_confirm: '',\n            }\n        },\n        methods: {\n            async handleSubmit(){\n               await axios.post('v1/register', {\n                    name: this.name,\n                    surname: this.surname,\n                    email: this.email,\n                    password: this.password,\n                    password_confirm: this.password_confirm\n\n                });\n                /* const response =  console.log(response); */\n                this.$router.push('/login');    \n               \n\n                \n                    \n\n            }\n        }\n    }\n</script>\n\n<style scoped>\nform {\n    padding: 30px;\n}\n    /* .auth-inner {\n        margin: 0;\n    } */\n</style>\n"]}]}